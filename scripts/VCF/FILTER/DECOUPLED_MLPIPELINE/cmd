nextflow run MLfilter_normalize.nf --vcf /homes/ernesto/lib/igsr_analysis_master/igsr_analysis/scripts/VCF/FILTER/DECOUPLED_MLPIPELINE/data/filt_0_1.NA12878.onlyvts.vcf.gz --prefix chr1_out -with-singularity /hps/nobackup/production/reseq-info/ernesto/GGVP/FILTERING/BCFTOOLS/variant_filtering.simg

